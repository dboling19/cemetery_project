<script>

  let owner_count = 1;

  document.getElementById('addOwner').onclick = function () {
    let template = `

      <label class="required">First Name</label><br>
      <input id="lot_purchase_form_owner" name="lot_purchase_form[owner][full_name]">

      <label>Street Address</label><br>
      <input id="lot_purchase_form_street_address" name="lot_purchase_form[owner][street_address]">

      <label>City</label><br>
      <input id="lot_purchase_form_city" name="lot_purchase_form[owner][city]">

      <label>State</label><br>
      <input id="lot_purchase_form_state" name="lot_purchase_form[owner][state]">

      <label>Zip Code</label><br>
      <input id="lot_purchase_form_zip_code" name="lot_purchase_form[owner][zip_code]">

      <label>Phone Number</label><br>
      <input id="lot_purchase_form_phone_num" name="lot_purchase_form[owner][phone_num]">`;

    let container = document.getElementById('lot_purchase_form_phone_num');
    let div = document.createElement('div');
    div.innerHTML = template;
    container.appendChild(div);

    owner_count++;

  }

</script>